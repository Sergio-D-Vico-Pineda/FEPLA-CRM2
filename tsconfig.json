{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@comp/*": [
        "./src/components/*"
      ],
      "@r/*": [
        "./*"
      ],
      "@db/*": [
        "./prisma/*"
      ],
      "@lay/*": [
        "./src/layouts/*"
      ],
      "@utils": [
        "./src/pages/api/utils.js"
      ]
    }
  }
}